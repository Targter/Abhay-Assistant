{
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/apprun.py"
    }
  ],
  "functions": {
    "api/apprun.py": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "ignore": ["data/", "faiss_index/", "venv/", ".env", "*.log"]
}
